    phy-reset-active-low;
};  
  
&gem3 {
    local-mac-address = [00 0a 35 00 01 25];
    phy-handle = <&phy3>;
    phy-mode = "gmii";
    status = "okay";
    phy-reset-gpio = <&gpio 419 1>;
    phy-reset-active-low;
};  
  

